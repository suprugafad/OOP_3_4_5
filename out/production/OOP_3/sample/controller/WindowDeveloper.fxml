<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="466.0" prefWidth="214.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.ControllerWindowDeveloper">
    <TextField fx:id="inputFirstname" layoutX="18.0" layoutY="48.0" promptText="Firstname">
        <font>
            <Font size="14.0" />
        </font>
    </TextField>
    <TextField fx:id="inputLastname" layoutX="18.0" layoutY="96.0" promptText="Lastname">
        <font>
            <Font size="14.0" />
        </font>
    </TextField>
    <ChoiceBox fx:id="choiceBox" layoutX="18.0" layoutY="223.0" prefHeight="25.0" prefWidth="175.0" />
    <TextArea fx:id="inputDecryption" layoutX="14.0" layoutY="290.0" prefHeight="131.0" prefWidth="186.0" />
    <Label layoutX="67.0" layoutY="14.0" text="Developer">
        <font>
            <Font size="18.0" />
        </font>
    </Label>
    <Button fx:id="btnEdit" layoutX="6.0" layoutY="428.0" mnemonicParsing="false" onMouseClicked="#edit" prefHeight="30.0" prefWidth="200.0" text="Edit">
        <font>
            <Font size="14.0" />
        </font>
    </Button>
    <Button fx:id="btnAdd" layoutX="6.0" layoutY="428.0" mnemonicParsing="false" onMouseClicked="#add" prefHeight="30.0" prefWidth="200.0" text="Add" />
    <TextField fx:id="inputCurrentProject" layoutX="18.0" layoutY="146.0" promptText="Current project">
        <font>
            <Font size="14.0" />
        </font>
    </TextField>
    <Label layoutX="19.0" layoutY="203.0" text="Program language:">
        <font>
            <Font size="14.0" />
        </font>
    </Label>
</AnchorPane>
